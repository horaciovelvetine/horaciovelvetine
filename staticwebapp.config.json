{
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": [
			"/assets/*",
			"/*.css",
			"/*.js",
			"/*.png",
			"/*.jpg",
			"/*.jpeg",
			"/*.gif",
			"/*.svg",
			"/*.ico",
			"/*.woff",
			"/*.woff2",
			"/*.ttf",
			"/*.eot"
		]
	},
	"routes": [
		{
			"route": "/*",
			"serve": "/index.html",
			"statusCode": 200
		}
	]
}
